<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ShapeImageView">
        <attr name="shape_TopLeft" format="enum">
            <enum name="rounded" value="0" />
            <enum name="cut" value="1" />
        </attr>
        <attr name="shape_TopRight" format="enum">
            <enum name="rounded" value="0" />
            <enum name="cut" value="1" />
        </attr>
        <attr name="shape_BottomRight" format="enum">
            <enum name="rounded" value="0" />
            <enum name="cut" value="1" />
        </attr>
        <attr name="shape_BottomLeft" format="enum">
            <enum name="rounded" value="0" />
            <enum name="cut" value="1" />
        </attr>
        <attr name="shape_All" format="enum">
            <enum name="rounded" value="0" />
            <enum name="cut" value="1" />
        </attr>
        <!--设置角-->
        <attr name="shape_corner_TopLeft" format="dimension|fraction" />
        <attr name="shape_corner_TopRight" format="dimension|fraction" />
        <attr name="shape_corner_BottomLeft" format="dimension|fraction" />
        <attr name="shape_corner_BottomRight" format="dimension|fraction" />
        <attr name="shape_corner_All" format="dimension|fraction" />

    </declare-styleable>
    <!--drawableTextView-->
    <declare-styleable name="DrawableTextView">
        <attr name="leftDrawableWidth" format="dimension" />
        <attr name="leftDrawableHeight" format="dimension" />
        <attr name="rightDrawableWidth" format="dimension" />
        <attr name="rightDrawableHeight" format="dimension" />
        <attr name="topDrawableWidth" format="dimension" />
        <attr name="topDrawableHeight" format="dimension" />
        <attr name="bottomDrawableWidth" format="dimension" />
        <attr name="bottomDrawableHeight" format="dimension" />
        <attr name="StrokeWidth" format="float" />
    </declare-styleable>
</resources>